{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/public/build/assets/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/build/(.*)",
        "dest": "/public/build/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|map))",
        "dest": "/public/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/favicon.ico",
        "dest": "/public/favicon.ico"
      },
      {
        "src": "/robots.txt", 
        "dest": "/public/robots.txt"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
  ],
  "env": {
    "APP_NAME": "COLMAP App",
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_KEY": "base64:SGVsbG8gV29ybGQgVGhpcyBJcyBBIEZha2UgS2V5Zm9yVGVzdGluZw==",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array", 
    "QUEUE_CONNECTION": "sync",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/tmp/database.sqlite",
    "VIEW_COMPILED_PATH": "/tmp/views",
    "LOG_CHANNEL": "stderr",
    "COLMAP_SERVICE_URL": "https://python-colmap.vercel.app"
  }
}