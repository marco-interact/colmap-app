# 3D Visualization Platform Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1

# Backend Configuration
SECRET_KEY=your-secret-key-change-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Database Configuration
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=3d_platform
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# File Storage Configuration
UPLOAD_DIR=/app/uploads
OUTPUT_DIR=/app/output
TEMP_DIR=/app/temp
MAX_FILE_SIZE=524288000

# COLMAP Configuration
COLMAP_BINARY_PATH=/usr/local/bin/colmap
COLMAP_WORKSPACE_DIR=/app/colmap_workspace
COLMAP_MAX_IMAGE_SIZE=1600
COLMAP_QUALITY=medium

# Processing Configuration
FRAME_EXTRACTION_INTERVAL=1.0
MAX_CONCURRENT_JOBS=4
JOB_TIMEOUT=3600

# AWS Configuration (for production)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=

# CORS Configuration
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:8000




